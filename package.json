    "clean": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./public/jspm_packages",
    "start": "forever start --spinSleepTime 10000 server.js",
    "stop": "forever stop ./server.js",
    "restart": "npm run stop && npm start"
